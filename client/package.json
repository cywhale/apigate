{
  "name": "apigate_client",
  "version": "1.2.1",
  "license": "MIT",
  "main": "./src/index.js",
  "scripts": {
    "serve": "sirv build --cors --single --port 3000 --http2",
    "watch": "preact watch --config config/preact.config.js",
    "build": "cp config/template.html src/ && export NODE_ENV=production && export ENABLE_BROTLI=true && preact build --config config/preact.config.js --no-esm --sw --brotli true --template ./src/template.html",
    "dev": "preact watch --sw false",
    "lint": "eslint src",
    "test": "jest",
    "bundle_watch": "webpack-bundle-analyzer --port 4200 build/stats.json"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "babel-loader": "^8.2.5",
    "compression-webpack-plugin": "^6.1.1",
    "css-loader": "5.2.7",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "html-webpack-plugin": "4.5.2",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "less": "^4.1.3",
    "less-loader": "7.3.0",
    "node-sass": "7.0.1",
    "preact-cli": "3.4.1",
    "preact-render-to-string": "5.2.1",
    "sass-loader": "10.3.1",
    "sirv": "^1.0.7",
    "sirv-cli": "^1.0.8",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "4.2.3",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-merge": "^5.3.0",
    "workbox-background-sync": "^6.5.2",
    "workbox-cacheable-response": "^6.5.2",
    "workbox-core": "^6.5.2",
    "workbox-expiration": "^6.5.2",
    "workbox-precaching": "^6.5.2",
    "workbox-routing": "^6.5.2",
    "workbox-strategies": "^6.5.2",
    "xmlhttprequest": "^1.8.0",
    "zlib": "^1.0.5"
  },
  "dependencies": {
    "firebase": "9.9.2",
    "nanoid": "4.0.0",
    "preact": "10.10.1",
    "preact-router": "4.0.1",
    "react-dropdown-tree-select": "2.7.1",
    "universal-cookie": "^4.0.4"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
