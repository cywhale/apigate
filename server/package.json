{
	"name": "apigate",
	"type": "module",
	"version": "1.2.0",
	"description": "fastify API gateway with preact-client",
	"main": "index.js",
	"scripts": {
		"start": "node --es-module-specifier-resolution=node ./src/index.js",
		"dev": "nodemon --es-module-specifier-resolution=node ./src/index.js",
		"prod": "export NODE_ENV=production && export PORT=3000 && nodemon --es-module-specifier-resolution=node ./src/index.js",
		"dev-client": "export NODE_ENV=development && export PORT=3003 && nodemon --es-module-specifier-resolution=node ./src/index.js"
	},
	"keywords": [
		"fastify",
		"preact"
	],
	"repository": "https://github.com/cywhale/apigate",
	"author": "cywhale",
	"license": "MIT",
	"dependencies": {
		"desm": "^1.2.0",
		"fastify": "^3.28.0",
		"fastify-autoload": "^3.11.0",
		"fastify-env": "^2.1.1",
		"fastify-helmet": "^7.0.1",
		"fastify-plugin": "^3.0.1",
		"fastify-redis": "^4.3.3",
		"fastify-static": "^4.6.1",
		"fluent-json-schema": "^3.1.0",
		"fs": "^0.0.2",
		"knex": "^2.0.0",
		"mercurius": "^9.3.6",
		"mercurius-cache": "^1.7.0",
		"mongoose": "^6.2.10",
		"sequelize": "^6.19.0"
	},
	"devDependencies": {
		"nodemon": "^2.0.12",
		"tedious": "^14.4.0"
	}
}
