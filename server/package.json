{
	"name": "apigate",
	"type": "module",
	"version": "1.2.1",
	"description": "fastify API gateway with preact-client",
	"main": "index.js",
	"scripts": {
		"start": "export NODE_OPTIONS=--max_old_space_size=8192 && node --es-module-specifier-resolution=node ./src/index.js",
		"dev": "export NODE_OPTIONS=--max_old_space_size=8192 && nodemon --es-module-specifier-resolution=node ./src/index.js",
		"prod": "export NODE_OPTIONS=--max_old_space_size=8192 && export NODE_ENV=production && export PORT=3000 && nodemon --es-module-specifier-resolution=node ./src/index.js",
		"dev-client": "export NODE_OPTIONS=--max_old_space_size=8192 && NODE_ENV=development && export PORT=3003 && nodemon --es-module-specifier-resolution=node ./src/index.js"
	},
	"keywords": [
		"fastify",
		"preact"
	],
	"repository": "https://github.com/cywhale/apigate",
	"author": "cywhale",
	"license": "MIT",
	"dependencies": {
		"@fastify/autoload": "^4.0.1",
		"@fastify/cors": "^7.0.0",
		"@fastify/env": "^3.0.0",
		"@fastify/helmet": "^8.0.0",
		"@fastify/redis": "^5.0.0",
		"@fastify/static": "^5.0.1",
		"desm": "^1.2.0",
		"fastify": "^3.29.0",
		"fastify-plugin": "^3.0.1",
		"fluent-json-schema": "^3.1.0",
		"fs": "^0.0.2",
		"knex": "^2.0.0",
		"lru-cache": "^7.10.1",
		"mercurius": "^9.4.0",
		"mercurius-cache": "^1.7.1",
		"mongoose": "^6.3.2"
	},
	"devDependencies": {
		"nodemon": "^2.0.12",
		"tedious": "^14.4.0"
	}
}
